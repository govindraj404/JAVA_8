{
    "emname": "cmh-ems-spaceName",
    "namespace": "z/sap.ic.cmh.spaceName/-",
    "version": "1.1.0",
    "instanceType": "reuse",
    "authorities": ["$ACCEPT_GRANTED_AUTHORITIES"],
    "options": {
        "management": true,
        "messagingrest": true,
        "messaging": true
    },
    "rules": {
        "queueRules": {
            "publishFilter": [
                "${namespace}/*"
            ],
            "subscribeFilter": [
                "${namespace}/*"
            ]
        },
        "topicRules": {
            "publishFilter": [
                "${namespace}/*"
            ],
            "subscribeFilter": [
                "*"
            ]
        }
    }
}